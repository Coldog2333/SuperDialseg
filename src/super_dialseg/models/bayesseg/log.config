# Set root logger level to DEBUG and its only appender to A1.
#log4j.rootLogger=DEBUG, A1 ,R
log4j.rootLogger=DEBUG ,R
# A1 is set to be a ConsoleAppender.
#log4j.appender.A1=org.apache.log4j.ConsoleAppender

# A1 uses PatternLayout.
#log4j.appender.A1.layout=org.apache.log4j.PatternLayout
#log4j.appender.A1.layout.ConversionPattern=%-4r [%t] %l %-5p %c %x - %m%n
#log4j.appender.A1.layout.ConversionPattern=%-4r %d [%t] %-5p %c - %m%n

#R 
log4j.appender.R=org.apache.log4j.RollingFileAppender
log4j.appender.R.file=/dev/null

log4j.appender.R.MaxFileSize=3MB
log4j.appender.R.MaxBackupIndex=9


# R uses PatternLayout.
log4j.appender.R.layout=org.apache.log4j.PatternLayout
#log4j.appender.R.layout.ConversionPattern=%-4r [%t] %l %-5p %c %x - %m%n
log4j.appender.R.layout.ConversionPattern=%-4r %d [%t] %-5p %c - %m%n

